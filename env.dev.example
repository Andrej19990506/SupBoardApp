# Development переменные окружения

# Frontend URLs (через nginx proxy)
VITE_APP_API_URL=http://localhost/api
VITE_APP_WS_URL=ws://localhost/api/ws
ENV_TYPE=development
VITE_APP_DEBUG=true

# Внутренние Docker URLs (для сервисов)
API_URL=http://server:8000
WS_URL=ws://websocket:8001

# Node.js
NODE_ENV=development

# База данных
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
POSTGRES_DB=appsubboard
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_secure_password_here

# Push notifications (замените на свои ключи)
APPLICATION_SERVER_KEY=your_application_server_key_here

# Telegram Bot (получите в @BotFather)
BOT_TOKEN=your_telegram_bot_token_here
WEBHOOK_PATH=/api/telegram/wh_dev
USE_DATABASE=true
ENVIRONMENT=development

# SMS.ru API (получите на sms.ru)
SMS_RU_API_ID=your_sms_api_id_here

# Google OAuth (получите в Google Console)
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Разрешенные origins для dev
ALLOWED_ORIGINS=http://localhost:3000,http://localhost

# Debug
REACT_APP_DEBUG=true

# Безопасность (ОБЯЗАТЕЛЬНО ЗАМЕНИТЕ НА СВОИ КЛЮЧИ!)
JWT_SECRET_KEY=your_very_secure_jwt_secret_key_here_min_32_chars 